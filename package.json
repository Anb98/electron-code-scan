{
  "name": "code_scan",
  "version": "0.1.0",
  "private": true,
  "main": "index.js",
  "author": "Abdiel Martinez",
  "homepage": ".",
  "description": "Check duplicate codes",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^13.5.0",
    "antd": "^4.20.4",
    "date-fns": "^2.28.0",
    "electron-is-dev": "^2.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "prop-types": "^15.8.1",
